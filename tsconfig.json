{
    "compilerOptions": {
       "target": "ES5",
       "lib": [
        "DOM",
       "DOM.Iterable",
       "ESNext" 
       ],
       "allowJs": true,
       "skipLibCheck": true,
       "esModuleInterop": true,
       "allowSyntheticDefaultImports": true,
       "strict": true,
       "forceConsistentCasingInFileNames": true,
       "noFallthroughCasesInSwitch": true,
       "module": "esnext",
       "moduleResolution": "node",
       "resolveJsonModule": true,
       "noEmit": false,
       "jsx": "react-jsx"
    },
    "include": [
        "src"
    ]
}